                                                                <span class="value">1</span>
                                                            </li>
                                                        </ul>
                                                    </li>
                                                    <li class="product-order-qty">
                                                        <span class="prod-qty">
                                                            <label>Qty</label>
                                                            @{
                                                                inputValue = Convert.ToInt32(TempData["MKT-SFS-007-WPS-PST-00860-28"]);
                                                                TempData.Remove("MKT-SFS-007-WPS-PST-00860-28");
                                                                }
                                                            @Html.TextBox("MKT-SFS-007-WPS-PST-00860-28", inputValue, new { @type = "number", @class = "qty-input", @min = "0", @readonly="true" })
                                                            @{
                                                                inputValue = 0;
                                                            }
                                                        </span>
                                                    </li>
                                                </ul>
                                            </li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                        </article>
                    }

                </form>    
                <a name="products-submit"></a>
                <section class="products-add-to-bag">
                    <div class="inner-wrapper">
                        <button class="products-add" id="finish-prog-order" data-action="@Url.Action("AddItemsToBag", "BagSurface")">Add To Bag</button>
                    </div>
                </section>
	        </div>
			<input name='ufprt' type='hidden' value='' />
		</div>
	</section>
</section>

